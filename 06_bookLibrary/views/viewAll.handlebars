<h1>Here are your books</h1>
<div class="container">
    {{#each books}}
        <div class="book">
            <a href="/books/details/:{{this._id}}">
                <img class="bookPoster" src="{{this.url}}" />
            </a>
        </div>
    {{else}}
        <h1>There are no books yet!</h1>
    {{/each}}
</div>